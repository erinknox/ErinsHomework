<Window x:Class="ZipCodeTextBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZipCodeTextBox"
        mc:Ignorable="d"
        Title="Zip Code Check" Height="300" Width="700">
    <StackPanel Margin="0,92,-0.4,0.4">
        <TextBox x:Name="uxTextEnter" TextChanged="uxTextEnter_TextChanged" Margin="142,0,130.6,0"></TextBox>
        <Button x:Name="uxSubmit" Content="Submit" IsEnabled="False" Margin="301,0,296.4,0">
            <Button.Style>
                <Style TargetType="{x:Type Button}" />
            </Button.Style>
        </Button>
    </StackPanel>
</Window>
